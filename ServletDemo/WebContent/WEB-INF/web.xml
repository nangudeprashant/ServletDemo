<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ServletDemo</display-name>
  <welcome-file-list>
    <welcome-file>Index.html</welcome-file>
  </welcome-file-list>
    <servlet>
		<servlet-name>Account</servlet-name>
		<servlet-class>com.javalive.controller.AccountController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Account</servlet-name>
		<url-pattern>/Account</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Message</servlet-name>
		<servlet-class>com.javalive.controller.MessageController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Message</servlet-name>
		<url-pattern>/Message</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ServletLifeCycle</servlet-name>
		<servlet-class>com.javalive.controller.ServletLifeCycleController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletLifeCycle</servlet-name>
		<url-pattern>/ServletLifeCycleDemo</url-pattern>
	</servlet-mapping>
</web-app>